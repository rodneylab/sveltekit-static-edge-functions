{
	"name": "sveltekit-static-edge-functions",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:edge": "BROWSER=none pnpm netlify dev",
		"build": "vite build",
		"preview": "vite preview",
		"preview:edge": "BROWSER=none pnpm netlify dev -c \"pnpm run preview\" --targetPort 4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:css": "stylelint \"src/**/*.{css,svelte}\"",
		"format": "prettier --plugin-search-dir . --write .",
		"prettier:check": "prettier --check --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.6.5",
		"@fontsource/figtree": "^5.0.2",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "1.20.0",
		"@types/node": "^20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"commitlint": "^17.6.5",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"netlify-cli": "^15.2.0",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"stylelint": "^15.6.2",
		"stylelint-config-recommended": "^12.0.0",
		"stylelint-config-standard": "^33.0.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.9"
	},
	"type": "module",
	"peerDependencies": {
		"@types/node": "^18.11.0"
	}
}
